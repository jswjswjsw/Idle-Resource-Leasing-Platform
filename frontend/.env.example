# API 配置
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_WS_URL=http://localhost:3001

# 环境配置
REACT_APP_ENV=development

# 第三方服务配置
# 微信OAuth配置
REACT_APP_WECHAT_APP_ID=your-wechat-app-id

# GitHub OAuth配置
REACT_APP_GITHUB_CLIENT_ID=your-github-client-id

# 地图服务配置
REACT_APP_BAIDU_MAP_AK=your-baidu-map-api-key
REACT_APP_AMAP_KEY=your-amap-api-key

# 文件上传配置
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# 应用配置
REACT_APP_TITLE=闲置资源租赁平台
REACT_APP_VERSION=1.0.0

# 支付配置
REACT_APP_ALIPAY_PUBLIC_KEY=your-alipay-public-key

# 统计分析配置（可选）
REACT_APP_GA_TRACKING_ID=your-google-analytics-id
REACT_APP_BAIDU_TONGJI_ID=your-baidu-tongji-id

# 错误监控配置（可选）
REACT_APP_SENTRY_DSN=your-sentry-dsn

# 功能开关
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_NOTIFICATION=true
REACT_APP_ENABLE_PAYMENT=true
REACT_APP_ENABLE_MAP=true